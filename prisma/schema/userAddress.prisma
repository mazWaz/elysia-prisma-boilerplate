model UserAddress {
  id                    String          @id @default(ulid())
  address               String
  rt                    Int
  rw                    Int
  district              String
  city                  String
  province              String
  userId                String          @unique
  user                  User            @relation("UserAddress", fields: [userId], references: [id])
  createdAt             DateTime        @default(now())
  updatedAt             DateTime        @updatedAt
}